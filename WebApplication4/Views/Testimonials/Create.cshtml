@model WebApplication4.Models.Testimonial

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
	<meta charset="utf-8">
	<title>Form-v9 by Colorlib</title>
	<!-- Mobile Specific Metas -->
	<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
	<!-- Font-->
	<link rel="stylesheet" type="/regester/text/css" href="/css/nunito-font.css">
	<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
	<!-- Main Style Css -->
		<link href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700,800,900" rel="stylesheet">
	<link href="https://fonts.googleapis.com/css2?family=Arizonia&display=swap" rel="stylesheet">

	<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">

	<link rel="stylesheet" href="/assest/css/animate.css">
	
	<link rel="stylesheet" href="/assest/css/owl.carousel.min.css">
	<link rel="stylesheet" href="/assest/css/owl.theme.default.min.css">
	<link rel="stylesheet" href="/assest/css/magnific-popup.css">

	<link rel="stylesheet" href="/assest/css/bootstrap-datepicker.css">
	<link rel="stylesheet" href="/assest/css/jquery.timepicker.css">
	<!--kkkkkkkkkkkkkkkkkk-->

	<link rel="stylesheet" href="/assest/css/flaticon.css">
	<link rel="stylesheet" href="/assest/css/style.css">
    <link rel="stylesheet" href="/regester/css/style.css"/>
</head>
<body class="form-v9" style="background-image: url('/regester/images/form-v9.jpg')">
	<nav class="navbar navbar-expand-lg  ftco_navbar  ftco-navbar-light bg-dark " style="background-color:#343a40!important" id="ftco-navbar">
		<div class="container">
			<a class="navbar-brand" href="#">Pacific<span>Travel Agency</span></a>

			<div class="collapse navbar-collapse" id="ftco-nav">
				<ul class="navbar-nav ml-auto">	
					<li class="nav-item"><a href="https://localhost:44346/HomePage" class="nav-link">Home</a></li>
				  
				</ul>
			</div>
		</div>
	</nav>


<hr />
<div class="page-content py-5" style="background-image: url('/regester/images/form-v9.jpg');margin-top:60px">
		<div class="form-v9-content">
        <form asp-action="Create"  enctype="multipart/form-data" class="form-detail">
            <h1 class=" text-danger ">Create Testimonial</h1>
            <div class="form-row-total">				
				<div class="form-row">
                     <input asp-for="Description" class="input-text" placeholder="Description" required/>
                </div>
			</div>             
              <div class="form-row-total">				
				<div class="form-row">
                     <input asp-for="ImageFile" class="input-text" required/>
                </div>
			</div>
            <div class="form-group">
				<label>Rating Select</label>
                <select asp-for="Rate"  style="border: 3px solid #ccc;" placeholder="Your Name" required>
                    <option value="1">One Stars</option>
                    <option value="2">Two Stars</option>
                    <option value="3">Three Stars</option>
                    <option value="4">Foure Stars</option>
                    <option value="5">Five Stars</option>
                </select>
            </div>
            <div class="form-row-last">
                <input type="submit" value="Create"  class="register" />
            </div>
        </form>
    </div>
</div>

@*<div>
    <a asp-action="Index">Back to List</a>
</div>*@

</body>
</html>
